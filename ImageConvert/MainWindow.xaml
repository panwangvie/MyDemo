<Window x:Class="ImageConvert.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageConvert"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Image Name="Source" Grid.Column="0"></Image>
        <Image Name="GotoImage" Grid.Column="0"></Image>
        <Button Content="选择" Click="ButtonBase_OnClick" Grid.Row="1"></Button>
        <ListBox Name="imageFormt" Grid.Row="1" Grid.Column="1" Selected="Selector_OnSelected">
   
        </ListBox>
    </Grid>
</Window>
